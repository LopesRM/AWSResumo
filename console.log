<main>
        <div id="content-container">
            <!-- O conteúdo dinâmico será carregado aqui -->
        </div>
        <div class="navigation-buttons">
            <button id="prev-btn">Previous</button>
            <button id="next-btn">Next</button>
        </div>

        <!-- Seções para interação com o script -->
        <section id="intro" class="hidden">
            <div class="service">
                <h2>Bem-vindo à AWS Adventure</h2>
                <p>Explore os serviços da AWS e aprenda de forma interativa e divertida!</p>
            </div>
        </section>

        <section id="ec2" class="hidden">
            <div class="service">
                <h2>EC2 (Elastic Compute Cloud)</h2>
                <p>O Amazon EC2 fornece capacidade de computação escalável na nuvem. Ele permite criar e gerenciar instâncias de máquinas virtuais para executar suas aplicações.</p>
            </div>
        </section>
    </main>

    <script>
        const sections = ["intro", "ec2"];

        function showSection(index) {
            console.log(`Exibindo seção: ${sections[index]}`); // Log para depuração
            sections.forEach((sectionId, i) => {
                const section = document.getElementById(sectionId);
                if (section) {
                    if (i === index) {
                        section.classList.remove("hidden");
                        section.classList.add("shown");
                    } else {
                        section.classList.remove("shown");
                        section.classList.add("hidden");
                    }
                } else {
                    console.error(`Seção com ID "${sectionId}" não encontrada.`);
                }
            });
        }
    </script>